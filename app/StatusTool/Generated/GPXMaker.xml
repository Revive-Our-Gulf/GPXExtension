<?xml version="1.0" ?>

<class terminator="true">

<namespace>NVL_App</namespace>
<solution>F</solution>
<name>GPXMaker</name>
<description>Converts a set of statuses to a GPX file</description>

<includes>
    <include name="iostream" namespace="std" local="false" />
    <include name="Status.h" local="true" />
</includes>

<variables>
    <variable type="vector<Status *>" name="statuses" code="00" />
</variables>

<constructors>
    <constructor type="auto" inline="false" />
 </constructors>

<methods>

    <!-- Render XML-->
    <method section="Render" access="public" return="string" name="RenderXML" description="Render the GPX xml" inline="false" />
</methods>

</class>