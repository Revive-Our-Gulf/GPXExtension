    <variable type="double" name="depth" code="10" />
    <variable type="double" name="altitude" code="10" />
    <variable type="double" name="temperature" code="10" />
    <variable type="string" name="mode" code="10" />
    <variable type="int" name="satelliteCount" code="10" />
    <variable type="double" name="posCertainity" code="10" />
    <variable type="bool" name="velocityValid" code="10" />
